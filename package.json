{
  "name": "yaf-extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/falsefalse/yaf-extension.git"
  },
  "version": "1.1.4",
  "private": true,
  "scripts": {
    "_stash_lint_clean": "git stash -q && yarn -s lint && jake -q clean",
    "release:chrome": "yarn -s _stash_lint_clean && jake -q && open ./pkg",
    "release:firefox": "yarn -s _stash_lint_clean && jake -q firefox && open ./pkg",
    "ch": "DEV=1 jake -q",
    "ff": "DEV=1 jake -q firefox",
    "tc": "tsc --noEmit",
    "lint": "npm-run-all -pc --aggregate-output --silent format eslint",
    "eslint": "eslint --color src/*.ts Jakefile.js",
    "format": "prettier -u --ignore-path .gitignore --list-different ."
  },
  "devDependencies": {
    "@types/chrome": "^0.0.227",
    "@types/firefox-webext-browser": "^111.0.1",
    "@types/offscreencanvas": "^2019.7.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "lodash.template": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "typescript": "^5.0.3",
    "uglify-js": "^3.17.4"
  }
}
