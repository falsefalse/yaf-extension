{
  "name": "yaf-extension",
  "repository": "git@bitbucket.org:false/yet-another-flags-extension.git",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "release:chrome": "git stash && jake -q clean && jake -q && open ./pkg",
    "release:firefox": "git stash && jake -q clean && jake -q firefox && open ./pkg",
    "ch": "DEV=1 jake -q",
    "ff": "DEV=1 jake -q firefox",
    "lint": "npm-run-all -pc --aggregate-output --silent format eslint",
    "eslint": "eslint --color src/*.js Jakefile.js",
    "format": "prettier --list-different *.js *.md *.json src/*.js src/popup.html"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "lodash.template": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "uglify-js": "^3.17.4"
  }
}
