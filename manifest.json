{
    "background_page": "service.html",
    "description": "Shows flag for the website in the location bar.",
    "icons": {
        "128": "img/icon/128.png",
        "48" : "img/icon/48.png",
        "32" : "img/icon/32.png",
        "16" : "img/icon/16.png"
    },
    "name": "Yet another flags",
    "content_scripts" : [
        {
            "matches" : ["http://*/*"],
            "js"      : ["js/get_loading_page_url.js"],
            "run_at"  : "document_start"
        }
    ],
    "page_action": {
        "default_icon": "img/icon/16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://api.ipinfodb.com/"
    ],
    "version": "0.8.0"
}